{
   "background": {
      "persistent": false,
      "scripts": [
         "background.js"
      ]
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Extension to quickly download GST returns from GST Portal",
   "icons": {
      "128": "images/gkbgst-logo-128.png",
      "16": "images/gkbgst-logo-16.png",
      "32": "images/gkbgst-logo-32.png",
      "48": "images/gkbgst-logo-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "GKB GST Assistant",
   "page_action": {
      "default_icon": {
         "128": "images/gkbgst-logo-128.png",
         "16": "images/gkbgst-logo-16.png",
         "32": "images/gkbgst-logo-32.png",
         "48": "images/gkbgst-logo-48.png"
      },
      "default_popup": "popup.html"
   },
   "permissions": [
      "activeTab",
      "declarativeContent",
      "downloads",
      "https://*.gst.gov.in/*"
   ],
   "short_name": "GKB GST",
   "version": "1.0.0"
}